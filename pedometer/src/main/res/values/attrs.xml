<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PMDailyRingChart">
        <attr name="EmptyAngle" format="float"/>
        <attr name="CircleInterval" format="dimension"/>
        <attr name="OuterCircleColor" format="color"/>
        <attr name="InnerCircleColor" format="color"/>
        <attr name="InnerCircleDashColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="XToolBarLayout">
        <attr name="xtblTitleText" format="string"/>
        <attr name="xtblTitleColor" format="color"/>
        <attr name="xtblTitleGravity" format="enum">
            <enum name="left" value="2"/>
            <enum name="center" value="4"/>
        </attr>
        <attr name="xtblShowTitleSpinner" format="boolean"/>
        <attr name="xtblBackgroundColor" format="color"/>
        <attr name="xtblShowDivider" format="boolean"/>
    </declare-styleable>
</resources>